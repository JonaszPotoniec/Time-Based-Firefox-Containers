{

  "manifest_version": 2,
  "name": "Time-base default containers",
  "version": "1.0",

  "description": "Let's you automagicaly change default containers based on time",


  "permissions": [
    "contextualIdentities",
    "cookies",
    "tabs",
    "bookmarks",
    "storage"
  ],

  "icons": {
    "48": "assets/icon.svg",
    "96": "assets/icon.svg"
  },

  "background": {
    "scripts": ["background/index.js"]
  },

  "options_ui": {
    "page": "options/options.html"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "ContainersToolkit@jonaszpotoniec.github.io"
    }
  }
}
